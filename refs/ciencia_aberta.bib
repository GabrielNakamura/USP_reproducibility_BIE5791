	file = {/Users/melinaleite/google_drive/p-biblioteca_zotero/zotero/storage/HIGXM9L3/Braga et al. - Not just for programmers How GitHub can accelerat.pdf;/Users/melinaleite/google_drive/p-biblioteca_zotero/zotero/storage/I644V6YW/2041-210X.html},
	issn = {2041-210X},
	journal = {Methods in Ecology and Evolution},
	keywords = {collaboration,data management,ecoinformatics,GitHub,open science,project management,reproducible research,version control},
	langid = {english},
