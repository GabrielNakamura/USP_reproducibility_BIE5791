	doi = {10.1111/geb.13381},
	file = {/Users/melinaleite/google_drive/p-biblioteca_zotero/zotero/storage/AXNJHC33/Mariano et al. - 2021 - LT-Brazil A database of leaf traits across biomes.pdf;/Users/melinaleite/google_drive/p-biblioteca_zotero/zotero/storage/KR772UVW/geb.html},
	issn = {1466-8238},
