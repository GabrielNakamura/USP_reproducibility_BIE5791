@misc{bledsoeDataRescueSaving2021,
	abstract = {1.	Historical and long-term environmental datasets are imperative to understanding how natural systems respond to our changing world, setting baselines and establishing trajectories of change. Although immensely valuable, these data are ultimately at risk of being lost unless they are actively managed, curated, and eventually archived on data repositories.  2.	The practice of data rescue, which we define as identifying, preserving, and sharing valuable data and associated metadata at risk of loss, is an important means of ensuring the long-term viability and accessibility of such datasets. Improvements in policies and best practices around data management will hopefully limit the future need for data rescue; these changes, however, do not apply retroactively. While the concept of rescuing data is not new, the term lacks a formal definition, is often conflated with other terms (i.e., data reuse), and lacks general recommendations.  3.	Here, we outline seven key guidelines for effective rescue of historically-collected and unmanaged datasets. We discuss how to prioritize which datasets to rescue, form effective data rescue teams, prepare the data and related metadata, and ultimately archive and share the rescued data.  4.	In an era of rapid environmental change, the best policy solutions will require evidence from both contemporary and historical sources. It is, therefore, imperative that we identify and preserve valuable, at-risk environmental data before they are lost to science.},
	author = {Bledsoe, Ellen and Burant, Joey and Higino, Gracielle and Roche, Dominique and Binning, Sandra and Finlay, Kerri and Pither, Jason and Pollock, Laura and Sunday, Jennifer and Srivastava, Diane},
	doi = {10.32942/osf.io/ra6ze},
	file = {/Users/melinaleite/google_drive/p-biblioteca_zotero/zotero/storage/HNFDYSWF/Bledsoe et al. - 2021 - Data rescue saving environmental data from extinc.pdf},
	institution = {EcoEvoRxiv},
	keywords = {data archiving,Ecology and Evolutionary Biology,historical data,Life Sciences,long-term ecological data,long-term studies,open data,open science,transparency},
	langid = {american},
	month = oct,
	shorttitle = {Data Rescue},
	title = {Data Rescue: Saving Environmental Data from Extinction},
	urldate = {2021-10-31},
	year = {2021},
	bdsk-url-1 = {https://doi.org/10.32942/osf.io/ra6ze}}

@article{blischakCreatingSharingReproducible2019,
	abstract = {Making scientific analyses reproducible, well documented, and easily shareable is crucial to maximizing their impact and ensuring that others can build on them. However, accomplishing these goals is not easy, requiring careful attention to organization, workflow, and familiarity with tools that are not a regular part of every scientist's toolbox. We have developed an R package,               workflowr               , to help all scientists, regardless of background, overcome these challenges.               Workflowr               aims to instill a particular "workflow" --- a sequence of steps to be repeated and integrated into research practice --- that helps make projects more reproducible and accessible.This workflow integrates four key elements: (1) version control (via               Git               ); (2) literate programming (via R Markdown); (3) automatic checks and safeguards that improve code reproducibility; and (4) sharing code and results via a browsable website. These features exploit powerful existing tools, whose mastery would take considerable study. However, the               workflowr               interface is simple enough that novice users can quickly enjoy its many benefits. By simply following the               workflowr               ~"workflow", R users can create projects whose results, figures, and development history are easily accessible on a static website --- thereby conveniently shareable with collaborators by sending them a URL --- and accompanied by source code and reproducibility safeguards. The               workflowr               R package is open source and available on CRAN, with full documentation and source code available at               https://github.com/jdblischak/workflowr               .},
	author = {Blischak, John D. and Carbonetto, Peter and Stephens, Matthew},
	doi = {10.12688/f1000research.20843.1},
	file = {/Users/melinaleite/google_drive/p-biblioteca_zotero/zotero/storage/TMD23M8G/Blischak et al. - 2019 - Creating and sharing reproducible research code the workflowr way.pdf;/Users/melinaleite/google_drive/p-biblioteca_zotero/zotero/storage/P4L6F34K/index.html},
	issn = {2046-1402},
	journal = {F1000Research},
	langid = {english},
	month = oct,
	pages = {1749},
	title = {Creating and Sharing Reproducible Research Code the Workflowr Way},
	urldate = {2024-07-01},
	volume = {8},
	year = {2019},
	bdsk-url-1 = {https://doi.org/10.12688/f1000research.20843.1}}

